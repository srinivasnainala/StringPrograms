public class ToprintInAlphabeticalOrderofaString {
public static void main(String[] args) {
	String s="cdewtrrhgbfdhbab";
	char a[]=s.toCharArray();
	Arrays.sort(a);
	System.out.println("Printing a String in Alphabetical order :");
    System.out.println(a);
    System.out.println("Printing a String in Reverse order :");
    for(int i=a.length-1;i>=0;i--)
    {
    	System.out.print(a[i]);
    }
}
}
