public class ToCounttheVowelsandConsonantsinaString {
public static void main(String[] args) {
	String s="aeiouvasu213";
	int vowels=0,consonants=0;
for(int i=0;i<s.length();i++)
{
	s=s.toUpperCase();
	char ch=s.charAt(i);
	if(Character.isAlphabetic(ch))
	{
		if(ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
		{
			vowels++;
		}
		else
		{
			consonants++;
		}
	}
}
System.out.println("The count of the Vowels in a Given String is: "+vowels);
System.out.println("The count of the consonants in a given string is: "+consonants);
}
}
